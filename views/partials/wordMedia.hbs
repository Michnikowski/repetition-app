<div class="row justify-content-center text-center">
  <div class="col">
    <h5>Phonetic notation</h5>
  </div>
  <div class="col">
    <h5>Audio</h5>
  </div>
  <div class="col">
    <h5>Video</h5>
  </div>
  <div class="col">
    <h5>Membean</h5>
  </div>
</div>
<div class="row justify-content-center text-center">
  <div class="col">
    <h4>{{repetitionWord.phoneticNotation}}</h4>
  </div>
  <div class="col">
    {{#if repetitionWord.audioUrl}}
    {{> audioUrl}}
    {{/if}}
  </div>
  <div class="col">
    {{> videoUrl}}
  </div>
  <div class="col">
    <h4>
      {{#if repetitionWord.membean}}
      Yes
      {{else}}
      No
      {{/if}}
    </h4>
  </div>
</div>
